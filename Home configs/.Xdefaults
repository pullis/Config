## .Xdefaults

#xvt*.transparent: true
#! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
#URxvt*.shading: 110

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800
URxvt.geometry: 130x25

URxvt.imLocale: fi_FI.UTF-8
URxvt*font: xft:termsyn:pixelsize=12:antialias=true:hinting=true #Source Code Pro:pixelsize=12
#    URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=11:antialias=true:hinting=true
    URxvt.url-launcher:      /usr/bin/firefox-aurora
    URxvt.matcher.button:   1
    URxvt.urlLauncher:  xdg-open
    URxvt.matcher.button:   1
    URxvt.xftAntialias: true
    URxvt.perl-ext-common:  default,tabbedex
#    URxvt.scrollstyle:  plain
    URxvt.scrollBar: false   
    URxvt.secondaryScreen : true
    URxvt.scrollTtyOutput   :   false
    URxvt.scrollWithBuffer  :   true
    URxvt.scrollTtyKeypress :   true
#    URxvt.tabbed.tabbar-fg: 15
#    URxvt.tabbed.tabbar-bg: 0
    URxvt.tabbed.tab-fg:    15
    URxvt.tabbed.tab-bg:    8
    URxvt.cursorBlink:  true
    URxvt.cursorUnderline:  true

!Font
URxvt*font:             	-*-tamsyn-medium-r-normal-*-12-87-100-100-c-60-iso8859-1
URxvt*boldFont:         	-*-tamsyn-bold-r-normal-*-12-87-100-100-c-60-iso8859-1

URxvt*background: #0c0c0c
URxvt*foreground: #979797

# Colors
